	<recipe name="Sagmill: Slate">
	    <sagmilling energy="2000">
	      <input name="slate" />
	      <output name="techreborn:dust:15" amount="2" />
	      <output name="minecraft:clay_ball" amount="2" />
	      <output name="minecraft:clay_ball" chance="0.5" />
	    </sagmilling>
